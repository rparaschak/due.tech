---
import Button from '~/components/ui/Button.astro';

const {
  variant = 'primary',
  icon = 'tabler:calendar',
  href = '#contact',
  class: className = '',
  ...rest
} = Astro.props;
---

<Button
  variant={variant}
  text="Get Your AI Readiness Assessment"
  icon={icon}
  href={href}
  class={`whitespace-nowrap ${className}`}
  {...rest}
/>
